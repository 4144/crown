// Lua script to launch on boot
boot_script = "lua/game"

// Package to load on boot
boot_package = "boot"

// Linux-only configs
linux = {
	renderer = {
		window_width  = 1280
		window_height = 720
		vsync = true
	}
}

// Windows-only configs
windows = {
	renderer = {
		window_width  = 1280
		window_height = 720
		vsync = true
	}
}
